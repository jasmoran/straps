include "src/stdlib/stdlib.v10"

func void main()(CircularBuffer cb, Any a, String s) {
	cb = CircularBuffer_new(3)
	# cb = []

	a = "A"
	cb.push_front(a)
	# cb = [A]

	s = cb.peek()
	s.println()

	a = "B"
	cb.push_front(a)
	# cb = [B A]

	s = cb.peek()
	s.println()

	a = "C"
	cb.push_front(a)
	# cb = [C B A]

	s = cb.peek()
	s.println()

	a = "D"
	cb.push_front(a)
}
