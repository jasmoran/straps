include "src/stdlib/stdlib.v10"
include "src/language/Tokenizer.v10"

func void main()(Tokenizer tk, Token t) {
	tk = Tokenizer_new("STDIN", STDIN)
	loop {
		t = tk.load()
		if (t == EOF) break
		t.toString().print()
		" ".print()
		tk.load().toString().print()
		" ".print()
		tk.load().toString().print()
		" ".print()
		tk.load().toString().print()
		" ".print()
		tk.load().toString().println()

		# New Line
		tk.load()
	}
}
