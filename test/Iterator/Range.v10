include "src/stdlib/stdlib.v10"

func void main() {
	# Standard range
  for (num in Range_new(6)) num.toString().print()
	"\n".print()

	# Standard range reverse
  for (num in Range_new(6).reverse()) num.toString().print()
	"\n".print()

	# Offset range
	for (num in Range_new(8, 12)) num.toString().print()
	"\n".print()

	# Offset range reverse
  for (num in Range_new(8, 12).reverse()) num.toString().print()
	"\n".print()

	# Backwards range
	for (num in Range_new(22, 19)) num.toString().print()
	"\n".print()

	# Backwards range reverse
  for (num in Range_new(22, 19).reverse()) num.toString().print()
	"\n".print()
}
