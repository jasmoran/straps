func typing(node)(children) {
	var children = node_children(var node)
	node_type(var node)

	if (rax == nCLASS) {
		node_data(var node)
		map_set(types[0], rax, var node)
		return
	}

	typing_children(var children)

	return
}

func typing_children(children)(node) {
	var node = ll_first(var children)
	loop {
		if (var node == 0) {
			return
		}
		lln_payload(var node)
		typing(rax)
		var node = lln_next(var node)
	}

	return
}
